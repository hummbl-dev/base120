{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Base120 Artifact Schema v1.0.0",
  "type": "object",
  "required": ["id", "domain", "class", "instance", "models"],
  "properties": {
    "id": {
      "type": "string",
      "minLength": 1
    },
    "domain": {
      "type": "string",
      "minLength": 1
    },
    "class": {
      "type": "string",
      "minLength": 1
    },
    "instance": {
      "type": "string",
      "minLength": 1
    },
    "models": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "string",
        "minLength": 1
      }
    }
  },
  "additionalProperties": false
}


